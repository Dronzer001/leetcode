class Solution {
public:
    int centeredSubarrays(vector<int>& nums) {
        int n=nums.size();
        int ans=0;
        for(int i=0;i<n;i+=1){
            for(int j=i;j<n;j+=1){
                int sum=0;
                for(int k=i;k<=j;k+=1){
                    sum+=nums[k];
                }
                for(int k=i;k<=j;k+=1){
                    if(nums[k]==sum){ans+=1; break;}
                }
            }
        }
        return ans;
    }
};
