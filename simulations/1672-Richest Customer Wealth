class Solution {
public:
    int maximumWealth(vector<vector<int>>& accounts) {
        int m=accounts.size();
        int n=accounts[0].size();
        int sum=INT_MIN;
        for(int i=0;i<m;i+=1){
            int sum1=0;
            for(int j=0;j<n;j+=1) sum1+=accounts[i][j];
            sum=max(sum,sum1);
        }
        return sum;
    }
};
