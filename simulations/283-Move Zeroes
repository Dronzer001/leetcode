class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int start=0;
        for(int i=0;i<nums.size();i+=1){
            if(nums[i]==0) continue;
            else{
                while(nums[start]!=0 && start<i) start+=1;
                swap(nums[i],nums[start]);

            }
        }
    }
};
