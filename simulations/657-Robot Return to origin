class Solution {
public:
    bool judgeCircle(string moves) {
        int x=0,y=0;
        for(int i=0;i<moves.length();i+=1){
            if(moves[i]=='L') y-=1;
            else if(moves[i]=='R') y+=1;
            else if(moves[i]=='U') x-=1;
            else x+=1;
        }
        return x==0 && y==0;
    }
};
