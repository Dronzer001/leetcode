class Solution {
public:
    long long removeZeros(long long n) {
        string str=to_string(n);
        string s="";
        for(int i=0;i<str.length();i+=1){
            if(str[i]!='0') s+=str[i];
        }
        long long ans=0;
        if(s.length()>0) ans=stoll(s);
        return ans;
    }
};Â©leetcode
