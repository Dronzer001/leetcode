class Solution {
public:
    vector<int> findMissingElements(vector<int>& nums) {
        int n=nums.size();
        vector<int>ans;
        sort(nums.begin(),nums.end());
        int k=nums[0];
        int i=0;
        while(i<n){
            if(k==nums[i]){
                k+=1;
                i+=1;
            }
            else{
                ans.push_back(k);
                k+=1;
            }
        }
        return ans;
    }
};
