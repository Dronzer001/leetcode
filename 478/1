class Solution {
public:
    int countElements(vector<int>& nums, int k) {
        int n=nums.size();
        sort(nums.begin(),nums.end());
        int ans=0;
        for(int num:nums){
            int greater=n- (upper_bound(nums.begin(),nums.end(),num)-nums.begin());
            if(greater>=k) ans+=1;
        }
        return ans;
    }
};
