class Solution {
public:
    long long lastInteger(long long n) {
        long long start=1,step=1,remain=n,left=true;
        while(remain >1){
            if(!left && (remain%2==0)) start+=step;
            remain=(remain+1)/2;
            step *=2;
            left=!left;
            cout<<start<<" "<<remain<<" "<< step<<"\n";
        }
        return start;
    }
};
