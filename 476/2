class Solution {
public:
    int minLengthAfterRemovals(string s) {
        int n=s.length();
        stack<char>st;
        for(int i=0;i<n;i+=1){
            if(!st.empty() && st.top()!=s[i]){
                st.pop();
            }
            else st.push(s[i]);
        }
        return st.size();
    }
};Â©leetcode
