class Solution {
public:
    long long countDistinct(long long n) {
        string s=to_string(n);
        int s1=s.length();
        long long sum=0;
        for(int i=1;i<s1;i+=1) sum+=pow(9,i);
        for(int i=0;i<s1;i+=1){
            int d=s[i]-'0';
            int first=max(0,d-1);
            sum+=first*pow(9,s1-i-1);
            if(d==0) break;
            if(i==s1-1) sum+=1;
        }
        return sum;
    }
};Â©leetcode
