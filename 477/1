class Solution {
public:
    long long sumAndMultiply(int n) {
        string s=to_string(n);
        string s1="";
        long long sum=0;
        for(int i=0;i<s.length();i+=1){
            if(s[i]!='0'){
                s1+=s[i];
                sum+=s[i]-'0';
            }
        }
        if(s1=="")return 0;
        return sum*stoi(s1);
    }
};
